{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "LettuceEncrypt.Internal.FileSystemAccountStore": "Trace"
    }
  },
  "LettuceEncrypt": {
    // Set this to automatically accept the terms of service of your certificate authority.
    // If you don't set this in config, you will need to press "y" whenever the application starts
    "AcceptTermsOfService": true,

    // You must at least one domain name
    "DomainNames": [ "eb44-2001-569-74ed-5e00-ab-54fd-786c-c710.ngrok.io" ],
    "UseStagingServer": true, //<-- use Let's encrypt staging server
    "AllowedChallengeTypes": "Http01",
    // You must specify an email address to register with the certificate authority
    "EmailAddress": "yours.tester@gmail.com"
  },

  "FileSystemStoreOptions": {
    // If path is not rooted(absolute) it will be treated as relative to user's home directory
    "path": ".LettuceEncrypt_Dev1",
    "password": "" // put strong password here or in secrets provider
  }
}
